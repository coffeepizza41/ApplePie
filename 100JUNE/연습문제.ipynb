    "N = int(input())\n",
    "newn = 0; tmp = N; i=0;\n",
    "\n",
    "while newn != N:\n",
    "    sumn = (tmp//10) + (tmp%10) \n",
    "    newn = (tmp%10)*10 + (sumn%10)\n",
    "    tmp = newn\n",
    "    i += 1\n",
    "    \n",
    "print(i)    "
