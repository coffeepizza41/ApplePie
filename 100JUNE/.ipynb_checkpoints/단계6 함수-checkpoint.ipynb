    "# 풀이\n",
    "a = [1,2,3,4,5]\n",
    "def solve(li):\n",
    "    return(sum(li))\n",
    "\n",
    "solve(a)\n",
    "\n",
    "# cf. 입력값의 개수를 모를 때\n",
    "def addn(*n): \n",
    "    res = 0\n",
    "    for i in n:\n",
    "        res += i\n",
    "    return res\n",
    "        \n",
    "addn(1,2,3)"
